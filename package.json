        "sass-loader": "^8.0.2",
        "vue-cli-plugin-netlify-lambda": "~0.1.1",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "plugin:vue/essential",
            "plugin:prettier/recommended",
            "eslint:recommended"
        ]
    },
    "husky": {
